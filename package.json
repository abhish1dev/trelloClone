{
  "name": "trello_clone",
  "version": "1.0.0",
  "description": "Creating a replica of trello where user can add boards, invite user, add tickets etc.",
  "main": "app.js",
  "author": "Abhishek Gangrade <abhishekgangrade92@gmail.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=8.16.0",
    "npm": ">=5.0.0",
    "yarn": ">=0.20.0"
  },
  "scripts": {
    "start": "gulp serve",
    "build": "gulp",
    "lint": "esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "lint:fix": "yarn lint -- --fix",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --require babel-core/register ./server/Tests/test.js",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors --require babel-core/register ./server/**/*.test.js",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "async": "^2.6.1",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-busboy": "0.0.2",
    "connect-multiparty": "^2.2.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "^2.6.9",
    "express": "^4.16.4",
    "express-validation": "^1.0.2",
    "express-winston": "^2.6.0",
    "formidable": "^1.2.1",
    "hat": "0.0.3",
    "helmet": "^3.15.0",
    "http-status": "^1.3.1",
    "jimp": "^0.6.0",
    "joi": "^10.6.0",
    "jsonwebtoken": "^8.4.0",
    "method-override": "^2.3.10",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.4",
    "nodemailer": "^4.7.0",
    "path": "^0.12.7",
    "sequelize": "^6.0.0",
    "underscore": "^1.9.1",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^3.2.4",
    "del": "^2.2.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-fix": "^1.0.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "^6.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.3",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.4.0",
    "gulp-nodemon": "^2.4.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-util": "^3.0.8",
    "istanbul": "^0.4.5",
    "jasmine": "^3.3.0",
    "jshint": "^2.10.2",
    "mocha": "^5.2.0",
    "run-sequence": "^1.2.2",
    "supertest": "^4.0.2",
    "validate-commit-msg": "^2.14.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
